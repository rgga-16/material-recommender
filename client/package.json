{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "watch": "npm-watch"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-loading-spinners": "^0.3.4",
    "three": "^0.152.2"
  },
  "dependencies": {
    "@threlte/core": "^6.0.0-next.7",
    "@threlte/extras": "^5.0.0-next.12",
    "@threlte/rapier": "^1.0.0-next.2",
    "@threlte/theatre": "^0.1.0-next.1",
    "npm-watch": "^0.11.0",
    "sirv-cli": "^2.0.0",
    "svelte-number-spinner": "^0.8.0",
    "svelte-range-slider-pips": "^2.1.1"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx,svelte"
    }
  }
}
